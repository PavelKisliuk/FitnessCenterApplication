<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="by.epam.javatraining.pavelkisliuk.controller.MyConsoleController">
    <Pane>
        <TextArea fx:id="mainTextArea" editable="false" focusTraversable="false" prefHeight="650.0" prefWidth="600.0">
            <font>
                <Font size="12.0" />
            </font>
        </TextArea>
       <Button fx:id="onTopButton" disable="true" layoutX="153.0" layoutY="667.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="On top" />
      <Button fx:id="exchangeButton" disable="true" layoutX="258.0" layoutY="667.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Exchange" />
      <Button fx:id="deleteButton" disable="true" layoutX="359.0" layoutY="667.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Delete" />
      <Button fx:id="addButton" layoutX="50.0" layoutY="667.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Add" />
      <Button fx:id="clearButton" layoutX="545.0" layoutY="673.0" mnemonicParsing="false" text="Clear" />
      <Button fx:id="showButton" disable="true" layoutX="455.0" layoutY="667.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Show" />
    </Pane>
    <padding>
        <Insets bottom="5.0" />
    </padding>
</AnchorPane>
